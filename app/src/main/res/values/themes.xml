<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Base application theme. -->
    <style name="Theme.CommonWebView" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">@color/skye_blue</item>
        <item name="colorPrimaryVariant">@color/sky_blue_700</item>
        <item name="colorOnPrimary">@color/sky_blue_200</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">@color/sky_blue_700</item>
        <item name="colorSecondaryVariant">@color/skye_blue</item>
        <item name="colorOnSecondary">@color/black</item>
        <item name="android:windowDisablePreview">true</item>
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>

        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">?attr/colorPrimaryVariant</item>

        <item name="imagetint">@color/white</item>
        <item name="colortext">@color/white</item>
        <item name="custom_color">@color/skye_blue</item>
        <item name="black_color_700">@color/black_color_700</item>
        <!-- Customize your theme here. -->
    </style>

